{
  "name": "Reports",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2-runtime ./bin/www.js",
    "dev": "nodemon ./bin/www",
    "test": "mocha 'test/**/*.js' --recursive --timeout 10000",
    "doc": "jsdoc -c ./.jsdoc.conf.json",
    "lint": "eslint .",
    "cont-int": "npm run-script lint",
    "fix-lint": "eslint . --fix",
    "lint-report": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o report.html"
  },
  "_moduleAliases": {
    "@models": "models",
    "@mongoose": "connection/mongoose",
    "@logger": "connection/logger",
    "@auth-proxy": "connection/proxy/auth",
    "@redis": "connection/redis",
    "@factory": "utils/factory"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run cont-int"
    }
  },
  "dependencies": {
    "@log4js-node/logstash-http": "^1.0.0",
    "actyv": "^1.0.2",
    "adler32-js": "^1.0.2",
    "aws-sdk": "^2.498.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^3.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "elasticsearch": "^16.1.1",
    "eureka-js-client": "^4.5.0",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "firebase-admin": "^8.7.0",
    "googleapis": "^52.1.0",
    "gstin-validator": "^0.2.5",
    "hashids": "^1.2.2",
    "http-status-codes": "^1.3.2",
    "ioredis": "^4.16.0",
    "lodash": "^4.17.15",
    "log4js": "^4.5.1",
    "module-alias": "^2.2.0",
    "moment": "^2.26.0",
    "mongoose": "^5.5.14",
    "mongoose-elasticsearch-xp": "^5.8.0",
    "mongoose-unique-validator": "^2.0.3",
    "node-schedule": "^1.3.2",
    "node-schedule-tz": "^1.2.1-4",
    "node-vault": "^0.9.16",
    "normalize-port": "^1.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.2.1",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "socket.io-pm2": "^1.0.0",
    "socketio-sticky-session": "^0.4.3",
    "socketio-with-pm2": "^0.1.8",
    "tinyurl": "^1.1.4",
    "uuidv4": "^6.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "mocha-simple-html-reporter": "^1.1.0",
    "nodemon": "^1.19.1",
    "eslint": "^6.8.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-no-unsanitized": "^3.1.0",
    "eslint-plugin-security": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "husky": "^4.2.3"
  }
}
